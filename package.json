  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc:check": "tsc --noEmit",
    "start": "node dist/server.js",
    "babelBuild": "node build.js",
    "build": "pnpm tsc --noEmit && pnpm babelBuild",
    "dev": "nodemon",
    "createModule": "node generateFilesOop.js",
    "nodemon": "nodemon",
    "lint:check": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\" --ignore-path .prettierignore",
    "prettier:fix": "prettier --write .",
    "lint-prettier": "pnpm lint:check && pnpm prettier:check",
    "prepare": "husky",
    "pg": "rimraf ./node_modules/prisma/query_engine-windows.dll.node && npx prisma generate",
    "pm": "npx prisma migrate dev",
    "ppush": "npx prisma db push",
    "ppull": "npx prisma db pull",
    "dev1": "ts-node-dev --respawn --transpile-only src/server.ts",
    "devmac": "ts-node-dev --respawn --transpile-only --exit-child --watch src src/server.ts",
    "gitpush_bash": "bash ./scripts/gitpush.sh",
    "gitpush": "pwsh -NoProfile -File ./scripts/gitpush.ps1",
    "migrate:dev": "DATABASE_URL='postgresql://postgres:112233@localhost:5433/ota?schema=auth,public,wallet' prisma migrate dev"
  },
  "lint-staged": {
    "src/**/*.ts": "pnpm lint-prettier"
  },
  "prisma": {
    "schema": "./prisma"
  },
